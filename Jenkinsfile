pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo "building"'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "deploying"'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "testing"'
            }
        }
        stage('Release') {
            steps {
                sh 'echo "releasing"'
            }
        }
    }
}